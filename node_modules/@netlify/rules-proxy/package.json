{
  "name": "@netlify/rules-proxy",
  "version": "0.1.4",
  "description": "This is a node middleware that works with express servers, connetct servers, etc, and implementes the full Netlify edge rules engine for your local dev environment.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "git push && git push --tags && gh-release",
    "version": "auto-changelog -p --template keepachangelog && git add CHANGELOG.md"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chokidar": "^2.0.4",
    "cookie": "^0.3.1",
    "http-proxy-middleware": "^0.19.1",
    "netlify-cli-logo": "^1.0.0",
    "netlify-redirect-parser": "^1.0.3",
    "netlify-redirector": "^0.0.4"
  },
  "devDependencies": {
    "auto-changelog": "^1.13.0",
    "gh-release": "^3.5.0",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "prettier --trailing-comma es5 --single-quote --write",
        "git add"
      ],
      "ignore": [
        "**/dist/*, **/node_modules/*"
      ]
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/netlify/netlify-rules-proxy.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/netlify/netlify-rules-proxy/issues"
  },
  "homepage": "https://github.com/netlify/netlify-rules-proxy#readme"
}
